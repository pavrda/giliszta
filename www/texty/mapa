function initialize() {
  var fenway = new google.maps.LatLng(50.116764,14.411002);
  
  var panoramaOptions = {
    position: fenway,
    pov: {
      heading: 34,
      pitch: 10,
	  zoom: 0

    }
  };
  var panorama = new  google.maps.StreetViewPanorama(document.getElementById('zooprahastreetview'),panoramaOptions);
  map.setStreetView(panorama);
}

google.maps.event.addDomListener(window, 'load', initialize);

    </script>

